#!/bin/bash
docker rmi -f icpe

rm -r learner data requirements.txt install-packages.R
cp -r ../learner .

mkdir data
cp -r ../data/* data/
cp -r ../requirements.txt .
cp -r ../install-packages.R .
docker-compose build

docker build -t frbl/icpe-machine-learning .

rm -r learner data requirements.txt install-packages.R
